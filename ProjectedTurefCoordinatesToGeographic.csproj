<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<PackageId>ProjectedTurefCoordinatesToGeographic</PackageId>
		<Version>1.0.0</Version>
		<Authors>Oğuzhan Candemir</Authors>
		<Description>Projected TUREF(Turkish Referece Frame) Coordinates to Geographical Coordinates</Description>
		<PackageTags>geo;projection;coordinate;geospatial;spatial</PackageTags>
		<RepositoryUrl>https://github.com/yourrepo/yourproject</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="NetTopologySuite" Version="2.6.0" />
		<PackageReference Include="ProjNet" Version="2.0.0" />
	</ItemGroup>

</Project>
